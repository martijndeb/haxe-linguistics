class linguistics.languages.Dutch implements linguistics.languages.ILanguage{
	public new(method) : Void -> Void

	 = function() = {
		this.basicStemmer = cast linguistics.languages.nl.PorterStemmerNL;
		this.token = cast linguistics.tokenizers.tokens.Token;
		this.lengthBasedTokenizer = cast linguistics.languages.nl.LengthBasedTokenizerNL;
		this.basicTokenizer = cast linguistics.languages.nl.BasicTokenizerNL;
		this.name = "Dutch";
	};

	public name(var) : String;

	public basicTokenizer(var) : Class<Dynamic>;

	public lengthBasedTokenizer(var) : Class<Dynamic>;

	public token(var) : Class<Dynamic>;

	public basicStemmer(var) : Class<Dynamic>;

	public stopwords(var) : Array<linguistics.tokenizers.tokens.IToken>;

	public createTokens(method) : Void -> Void

	 = function() = {
		var tokenizer = Type.createInstance(this.basicTokenizer,[]);
		this.stopwords = tokenizer.tokenize("aan af al alles als altijd andere ben bij daar dan dat de der deze die dit " + "doch doen door dus een eens en er ge geen geweest haar had heb hebben heeft " + "hem het hier hij  hoe hun iemand iets ik in is ja je  kan kon kunnen maar me " + "meer men met mij mijn moet na naar niet niets nog nu of om omdat ons ook op " + "over reeds te tegen toch toen tot u uit uw van veel voor want waren was wat " + "we wel werd wezen wie wij wil worden zal ze zei zelf zich zij zijn zo zonder zou");
	};

	static public basicTokenExpression(var) : EReg

	 = new EReg("[^a-zA-Z0-9_\\-'âêîôûàèìòùëïöüŸáéíóúýÂÊÎÔÛÀÈÌÒÙËÏÖÜŸÁÉÍÓÚÝ]+","g");

}