class linguistics.stemmers.Region12{
	public new(method) : myVowels : Array<String> -> Void

	 = function(myVowels:Array<String>) = {
		this.vowels = [];
		this.vowels = myVowels;
	};

	vowels(var) : Array<String>;

	public getRegion1(method) : myString : String -> String

	 = function(myString:String) = {
		if ((myString.length < 4))return "");
		myString = myString.toLowerCase();
		var pos = 1;
		while ((pos < myString.length)) {
			if ((this.vowels.indexOf(myString.substr(pos - 1,1),null) > -1 && this.vowels.indexOf(myString.substr(pos,1),null) == -1))return myString.substr(pos + 1,null));
			pos ++;
		};
		return "";
	};

	public getRegion2(method) : myString : String -> R1 : String -> String

	 = function(myString:String,R1:String) = {
		if ((R1.length < 2))return "");
		myString = myString.toLowerCase();
		var pos = myString.indexOf(R1,null);
		while ((pos < myString.length)) {
			if ((this.vowels.indexOf(myString.substr(pos - 1,1),null) > -1 && this.vowels.indexOf(myString.substr(pos,1),null) == -1))return myString.substr(pos + 1,null));
			pos ++;
		};
		return "";
	};

}